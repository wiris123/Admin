					// 검색어 파라미터 가져오기
					var search = (getHashParameter()['searchText'] == undefined ? '' : getHashParameter()['searchText']);
					var node = (getHashParameter()['searchNode'] == undefined ? domainNodeId : getHashParameter()['searchNode']);

					// 페이징 처리
					page = (getHashParameter()['page'] == undefined ? 1 : getHashParameter()['page']);
					/*
					$('#paging').pagination(totalCount, {
						num_display_entries: 5, // 페이징 출력 갯수
						num_edge_entries: 1, // 페이징이 많을 경우 앞,뒤 페이징 점프 출력 갯수
						items_per_page: pageSize, // 리스트 출력 갯수
						prev_text: '이전',
						next_text: '다음',
						ellipse_text: '',
						callback: handlePaginationClick, // 콜백
						link_to: '#list?searchNode=' + node + '&searchText=' + search + '&page=__id__', // 페이징 링크
						current_page: page - 1 // 현재 페이지
					});
					$('.paging').find('a:first').removeClass('current');
					$('.paging').find('a:last').removeClass('current');
					*/
					link = '#list?searchNode=' + node + '&searchText=' + search;
					if (totalCount > 10) {
				        $(".paging").paging(totalCount, {
				            format: '[< ncnnn >]',
				            perpage: 10,
				            page: page,
				            onFormat: function(type) {
				                switch (type) {
				                    case 'block': // n and c
				                    	if (this.value == page) {
				                        	return '<a href="' + link + '&page=' + this.value + '" class="current">' + this.value + '</a>';
				                    	} else {
				                    		return '<a href="' + link + '&page=' + this.value + '">' + this.value + '</a>';
				                    	}
				                    case 'next': // >
				                        return '<a href="' + link + '&page=' + this.value + '" class="btn next"><span>다음</span></a>';
				                    case 'prev': // <
				                        return '<a href="' + link + '&page=' + this.value + '" class="btn prev"><span>이전</span></a>';
				                    case 'first': // [
				                        return '<a href="' + link + '&page=' + this.value + '" class="btn first"><span>처음</span></a>';
				                    case 'last': // ]
				                        return '<a href="' + link + '&page=' + this.value + '" class="btn last"><span>마지막</span></a>';
				                }
				            }
				        });
					} else {
						$(".paging").empty();
					}
				}
			},
			error : function(e) {
				alert('서버와 통신 중 오류가 발생했습니다: ' + e);
			}
		});
	}
