<script type="text/javascript">
	var gv_eventId = '1887';
	
	$(function() {
		//	메일 서비스 선택
		$("#chooseMail").on("change", function() {
			var selected = $(this).find("option:selected").text();
			var obj = $("#sendEmailMail2");

			if (selected == "직접입력") {
				obj.attr("readonly", false);
				obj.val("");
				obj.focus();
			} else {
				obj.val(selected);
				obj.attr("readonly", true);
			}
		});

		//	이메일 팝업 (동의함)
		$("#sendMail").on("click", function() {
			sendEMail();
		});
	});

	//	메일보내기 화면 초기화
	function clearMail(title) {
		$("#chooseMail option:eq(0)").attr("selected", "selected");

		$("#eventApplyName").val("");
		$("#eventTelNum1").val("010");
		$("#eventTelNum2").val("");
		
		$("#sendEmailMail1").val("");
		$("#sendEmailMail2").val("");
		$("#sendEmailMemo").val("");

		$("#strCount").text("0");

		$("label[for=sendEmailAgree1]").addClass("on");
		$("label[for=sendEmailAgree2]").removeClass("on");

		$("input:radio[name=agree]:input[value=1]").attr("checked", true);
		$("input:radio[name=agree]:input[value=2]").attr("checked", false);

		$("#mailTitle").text(title);
	}

	/* function CBSendMail() {
		$("#cancelMail").click();
	} */

	//	메일 전송
	function sendEMail() {

		var prdtType = $("#proType").val();
		var planSeq = $("#planSeq").val();
		var email1 = $("#sendEmailMail1").val();
		var email2 = $("#sendEmailMail2").val();
		var custMemo = $("#sendEmailMemo").val();

		if (planSeq =="" || planSeq == null){
			alert("간편설계 내용이 없습니다.");
			return;
		}
		
		if($('#eventJoin').is(':checked')){
			if(!validateEvent()){
				return false;
			}
		}

		//이메일 주소
		if (isEmptyValue("sendEmailMail1", "이메일 주소를 입력해 주세요.")) return false;
		if (isEmptyValue("sendEmailMail2", "이메일 주소를 입력해 주세요.")) return false;

		//이메일 주소 형식
		var emailAddress = email1 + '@' + email2;
		if (!isValidEmailAddress(emailAddress, "sendEmailMail1")) return false;

		//개인 정보 수집 및 이용 동의
		if ($("input[name=agree]:checked").val() == 2) {
			alert("메일 발송을 위한 개인 정보 수집 및 이용 동의 수신에 체크해주세요.");
			return false;
		}
		
		if($('#eventJoin').is(':checked')){
			if(!regMailEvent()){
				return false;
			}
		}

		var f = $("#mailFrm")[0];
		f.sendType.value = "mail";

		planSendMail(planSeq, email1, email2, custMemo, prdtType);
	}
	
	function regMailEvent() {
		var isSuccess = true;
		var param = {
			"eventId" : gv_eventId,
			'name' : $('#eventApplyName').val(),
			'tel1' : $('#eventTelNum1').val(),
			'phone' : $('#eventTelNum2').val(),
			'url' : '',
			'memo' : $('#sendEmailMemo').val(),
			'popEventApplyValue1' : '',
			'popEventApplyValue2' : '',
			'popEventApplyValue3' : '',
			'agreechk' : $('#eventAgree1').is(':checked') ? 'Y' : 'N'
		};
		
		$.ajax({
			type : 'POST',
			url : '/event/entryRegister.eds',
			data : param,
			async : false,
			dataType : 'json',
			success : function(data) {
				var result = data;
				if (!result.success) {
