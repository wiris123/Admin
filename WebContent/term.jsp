	// 30일 기준 만료일 가져오기
	function getExpireDay(regDate){
		var year  = parseInt(regDate.substring(0,4),10);
	    var month = parseInt(regDate.substring(4,6),10)-1;
	    var day   = parseInt(regDate.substring(6,8),10);

	    var regDay = new Date(year,month,day);
		var toDay = new Date();

		var betweenDay = parseInt(((toDay.getTime() - regDay.getTime())/1000/60/60/24),10);
		var	expireDay = (30 - betweenDay);

		return expireDay;
	}

