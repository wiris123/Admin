<script type="text/javascript">
// SMS 인증 관련 함수
// 인증SMS 전송
var sendMsg = false;	// SMS 전송여부
var isTimeout = true;	// 타임아웃 여부
var intervalFn = null;	// 인터벌 함수
var limitTime = 299;		// 타임아웃 초

function sendSmsCert(){

	var cellNo1 = $.trim($("#historyTelnum1").val());
	var cellNo2 = $.trim($("#historyTelnum2").val());

	if ($('#historyTelnum2').val().length < 7) {
		alert("휴대폰 번호를 바르게 입력했는지 확인해 주세요.");
		$('#historyTelnum2').focus();
		return;
	}

	if (cellNo1 == "" || cellNo2 == "" ){
		alert("휴대폰번호를 입력하세요");
		return;
	}

	var jsonData = '{';
	jsonData += '"phoneNo" : "'+cellNo1+cellNo2+'"';
	jsonData += '}';

	jQuery.ajax({
		type : "POST",
		url : "/sendSmsCert.eds",
		data : jsonData,
		dataType : 'json',
		success : function(result) {
			if (result.success) {
		    	sendMsg = true;
		    	isTimeout = false;
		    	limitTime = 299;
		    	// 함수 클리어
		    	clearInterval(intervalFn);
				// 잔여시간 처리
				intervalFn = setInterval(setTimeoutCert, 1000);
				// 인증입력영역 보이기
				$('#certInputArea').removeClass("none");
				$('#certInputNumber').val('');
				$('#certInputNumber').focus();
				// 타임아웃문구 히든처리
				$('#timeoutText').addClass("none");
			}
		},
		error : function() {}
	});
}
// 잔여시간 Interval 함수
function setTimeoutCert() {
	var min = parseInt(limitTime / 60);
	var sec = limitTime % 60;

	if (sec < 10) sec = "0" + sec;

	$("#timerTxt").text( min+":"+sec );

	if (limitTime <= 0) stopFn();

	limitTime --;
}
// Interval 스톱함수
function stopFn() {
	// Interval 함수 클리어
	clearInterval(intervalFn);
	// 타임아웃 문구 보이기
	$('#timeoutText').removeClass("none");
	// 타임아웃상태
	isTimeout = true;
}

