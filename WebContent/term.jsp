			//planData.annMoney = arryData.nowTime10;
			//planData.rtnRatio = arryData.returnRatio;
		}

		if (proType == '15'){
			var guaranteeArry = arryData.guaranteeArry;

			
			var val1 = 0;
			var val2 = 0;
			var val3 = 0;
			for (var j=0 ; j < guaranteeArry.length ; j++){
				var item = guaranteeArry[j];
				var amt = (item.amt).replace("만원", "").replace(",", "");
				
				if ((item.name).indexOf('틀니')>0){
					val1 = val1+ parseFloat(amt);					
				}
				if ((item.name).indexOf('브릿지')>0){
					val2 = val2+parseFloat(amt);					
				}
				if ((item.name).indexOf('임플란트')>0){
					val3 = val3+parseFloat(amt);					
				}				
			}
			
			planData.money = arryData.padSmtotPrm;
			planData.val1 = val1;
			planData.val2 = val2;
			planData.val3 = val3;
			planData.selImplStr = arryData.inputObj.selImplStr;
		}		
		
		// 계산 버튼 클릭시 입력 데이터를 쿠키 저장
		var cookieData = $.cookie("recentPlan");
		
		if (cookieData == null){
			// 쿠키값이 없으면 새로운 배열생성
			planData.id = 1;
			$.cookie("recentPlan","["+JSON.stringify(planData)+"]",{expires:7,path:'/'});
		}else{
			// 쿠키값이 있으면 기존 배열 맨위에 추가
			var cookieArry = $.parseJSON(cookieData);
			planData.id = (cookieArry[0].id+1);
			// 최대 6개까지만 저장
			if (cookieArry.length > 5){
				cookieArry.pop();
			}
			cookieArry.unshift(planData);

			$.cookie("recentPlan",JSON.stringify(cookieArry),{expires:7,path:'/'});
		}
		viewPlanCookie(''); // 갱신
	}


	//가설 쿠키아이디로 가설정보 가져오기
	function getPlanCookie(planCookId){
		// 최종 쿠키값
		var recentPlanCookie = $.cookie("recentPlan");
		var returnObj ="";
		if (recentPlanCookie == null){}else{
			var recentObj= $.parseJSON($.cookie("recentPlan"));
			for (var i=0 ; i < recentObj.length ; i++){
				if (recentObj[i].id == planCookId){
					returnObj = recentObj[i];
				}
			}
		}
		return returnObj;
	}

