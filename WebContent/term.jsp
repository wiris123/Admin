<script type="text/javascript">

	//가설 쿠키저장
	function setPlanCookie(result){
		var arryData = null;
		
		if(result.arryData.length > 1) {
			arryData = result.arryData[1];
		}else{
			arryData = result.arryData[0];
		}

		var proType;
		var planData;

		//	바로가입하기에서 오면 배열이 0
		if (typeof(arryData) == "undefined") {
			arryData = result.arryData;
		}
		
		proType = arryData.inputObj.proType;
		planData = {
				"proType" : proType,
				"planSeq" : arryData.inputObj.planSeq,
				"birth"   : arryData.inputObj.contBirth,
				"gender"  : arryData.inputObj.contGender,
				"insTerm" : arryData.inputObj.insuPeriod,
				"napTerm" : arryData.inputObj.payPeriod,
				"date"	  : getDateYMD(),
				"planType": arryData.inputObj.planType
			};

		if (proType == '1'){
			planData.money = arryData.padSmtotPrm;
			planData.gVal1 = arryData.gVal1;
			planData.gVal2 = arryData.gVal2;
			planData.gVal3_1 = arryData.gVal3_1;
			planData.gVal3_2 = arryData.gVal3_2;
			planData.gVal4 = arryData.gVal4;
			planData.gVal5 = arryData.gVal5;
		}
		if (proType == '2'){
			planData.money = arryData.padSmtotPrm;
			planData.deathAmt = arryData.deathAmt;
			planData.insType = arryData.inputObj.insrPrdTypCd;	// 보험기간유형코드
			planData.napType = arryData.inputObj.padPrdTypCd;	// 납입기간유형코드
		}
		if (proType == '3'){
			planData.money = arryData.padSmtotPrm;
			planData.death11 = arryData.deathAmt11;
			planData.death12 = arryData.deathAmt12;
			planData.death21 = arryData.deathAmt21;
			planData.death22 = arryData.deathAmt22;
			planData.death31 = arryData.deathAmt31;
			planData.death32 = arryData.deathAmt32;
			planData.dis1 = arryData.disAmt1;
			planData.dis2 = arryData.disAmt2;
			planData.ann1 = arryData.annuityAmt1;
			planData.ann2 = arryData.annuityAmt2;
		}
		if (proType == '4'){
			planData.money = arryData.totPremium;
			planData.rtnMoney = arryData.returnMoney;
			planData.rtnRatio = arryData.returnRatio;
		}
		if (proType == '5'){
			planData.money = arryData.totPremium;
			planData.taxMoney = getTaxMoney(arryData.totPremium)+"만"; //세액공제액
			planData.annAge  = arryData.inputObj.annuityAge;
			planData.annMoney = arryData.annuityMoney;
			planData.rtnRatio = arryData.returnRatio;
		}
		if (proType == '7'){
			planData.money = arryData.totPremium;
			planData.annAge  = arryData.inputObj.annuityAge;
			planData.annMoney = arryData.nowTime10;
			planData.rtnRatio = arryData.returnRatio;
		}
		if (proType == '8'){
			planData.money = arryData.totPremium;
			
			var yearPeriod = planData.napTerm + '년';
			
			// 투자수익율별 해지 환급금 예시갯수
			var startIdx = arryData.nowRateArry.length / 3 * lastSelectRate;
			var endIdx = startIdx + arryData.nowRateArry.length / 3;
			
			for(var i=startIdx; i<endIdx; i++){
				if(yearPeriod == arryData.nowRateArry[i].totTerm){
					planData.rtnMoney = arryData.nowRateArry[i].rtnMoney;
					planData.rtnRatio = arryData.nowRateArry[i].RtnRatio;
					break;
				}
			};
			
			if(lastSelectRate == 0){
				planData.stdRatio = '-1.0';
			}else if(lastSelectRate == 1){
				planData.stdRatio = '2.5';
			}else{
				planData.stdRatio = '3.75';
			}
		}
		
		if (proType == '9' || proType == '10' || proType == '11' || proType == '12' || proType == '13' || proType == '14' || proType == '13' || proType == '14'
			|| proType == '16' || proType == '17'){
			planData.money = arryData.totPremium;
			
			//실손개정추가 by syi
			planData.selectedInsuList = arryData.inputObj.selectedInsuList;
			
