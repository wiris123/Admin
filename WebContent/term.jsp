	//가설정보 쿠키로 최근 가설내역을 #recentPlanDiv 에 뿌리기
	function viewPlanCookie(type){
		// 최종 쿠키값
		var recentPlanCookie = $.cookie("recentPlan");
		var recentPlanCount = 0;
		var initHtml ='<p class="nodata">해당 보험의 최근설계내역이 없습니다.</p>';
		if (recentPlanCookie == null){
			$("#recentPlanDiv").html(initHtml);
			$("#recentPlanArea1").addClass("none");
			$("#recentPlanArea2").removeClass("none");
		}else{
			var recentObj= $.parseJSON($.cookie("recentPlan"));
			var recentObjData = new Array();

			//recentPlanCount = recentObj.length; // 전체카운트
			
			$.each(recentObj, function(){
				if(this.proType != '49' && this.proType != '50' && this.proType != '51'){
					recentPlanCount++;
				}  
			});

			// 검색 type 이 있는경우 처리
			if (type !=""){
				for (var i=0 ; i < recentObj.length ; i++){
					if(type=="9"){
						if (recentObj[i].proType == "9" || recentObj[i].proType == "10" || recentObj[i].proType == "11"
								|| recentObj[i].proType == "12" || recentObj[i].proType == "13" || recentObj[i].proType == "14" || recentObj[i].proType == "16" || recentObj[i].proType == "17"){
							recentObjData.push(recentObj[i]);
						}
					}else{
						if (recentObj[i].proType == type){
							recentObjData.push(recentObj[i]);
						}
					}
				}
				recentObj = recentObjData;
			}
			var makeHtml = '';
			if (recentObj.length > 0 ){
				makeHtml = makeRecentPlanHtml(recentObj);
			}else{
				makeHtml = initHtml;
			}

			$("#recentPlanDiv").html(makeHtml);
			$("#recentPlanArea1").removeClass("none");
			$("#recentPlanArea2").addClass("none");
		}
		// 최근설계 총카운트 세팅
		$("#recentPlanCount, #headerRecentPlanCount").html((recentPlanCount==0)?"":recentPlanCount);
		// UI Refresh
		contentReload();
		// 전체 선택
		if (type ==""){
			$("#uiRecentTab1").find('li').removeClass("on");
			$("#uiRecentTab1").find('li').first().addClass("on");
		}
	}

